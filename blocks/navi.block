<nav class="navi navi--main js-navi -column" data-content-field="navigation-navi">
    {.repeated section items}
        {.section collection}
            {.var @slug title|slugify}
            <a class="navi__a navi__a--{@slug} js-navi-info p a" data-slug="{@slug}" href="#{@slug}">{title}</a>
        {.end}
    {.end}
</nav>
