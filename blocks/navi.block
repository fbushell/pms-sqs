<nav class="navi navi--main js-navi -column" data-content-field="navigation-navi">
    {.repeated section items}
        {.section collection}
            {.var @slug title|slugify}
            <a class="navi__a navi__a--{@slug} js-navi-a js-navi--{@slug} p a" href="#{@slug}">{title}</a>
        {.end}
    {.end}
    <a class="navi__a navi__a--close js-navi-close p a" href="#">Close</a>
</nav>
